{
  "name": "@duetds/icons",
  "version": "3.0.8",
  "description": "This package includes Duet Design System Icons and related utilities.",
  "bugs": {
    "email": "duetdesignsystem@lahitapiola.fi"
  },
  "license": "SEE LICENSE IN LICENSE.md",
  "author": "LocalTapiola Services Ltd <duetdesignsystem@lahitapiola.fi>",
  "main": "lib/icons.js",
  "types": "lib/icons.d.ts",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "precommit": "npm run svglint",
    "postpublish": "rm ./src/assets/*.js ./src/icons.js ./src/icons.d.ts",
    "build": "run-s svgo pretest test svglint jsonlint prepublishOnly copy:build postpublish",
    "copy:build": "copyfiles --up 1 \"./src/**/*\" \"./lib\"",
    "jsonlint": "jsonlint ./src/_data/duet-icons.json -q -V .jsonlintschema",
    "svglint": "svglint ./src/assets/* --ci",
    "svgo": "svgo -f ./src/assets",
    "prepublishOnly": "node ./src/scripts/prepublish.js",
    "start": "npm run build",
    "test:ci": "npm run test -- --ci --reporters=default --reporters=jest-junit",
    "pretest": "npm run prepublishOnly",
    "test": "jest",
    "posttest": "npm run postpublish",
    "tags": "node ./src/scripts/tag-icons.js"
  },
  "jest": {
    "reporters": [
      "default"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/config/jestSetup.js"
    ]
  },
  "devDependencies": {
    "copyfiles": "2.4.1",
    "inquirer": "9.1.2",
    "jest": "28.1.3",
    "jest-image-snapshot": "5.2.0",
    "jest-junit": "14.0.1",
    "jsonlint2": "1.7.1",
    "npm-run-all": "4.1.5",
    "sharp": "0.31.1",
    "svglint": "2.1.1",
    "svgo": "2.8.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "8d18eefeb64ddce8e898b68eed12afe55197fe57"
}
